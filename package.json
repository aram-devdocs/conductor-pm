{
  "name": "client",
  "productName": "client",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "prepare": "husky",
    "prisma:generate": "cd python_service && poetry run prisma generate --schema ../prisma/schema.prisma",
    "prisma:migrate": "cd python_service && poetry run prisma migrate dev --schema ../prisma/schema.prisma",
    "python:setup": "cd python_service && poetry install",
    "python:dev": "cd python_service && poetry install && poetry run python -m src.agent",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "setup": "yarn install && yarn prisma:generate && cd python_service && poetry install && cd ..",
    "python:fix-lock": "cd python_service && poetry lock --no-update",
    "python:dev:fix": "yarn python:fix-lock && yarn python:dev",
    "clean": "rm -rf node_modules .webpack .out",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@electron-forge/cli": "^7.6.1",
    "@electron-forge/maker-deb": "^7.6.1",
    "@electron-forge/maker-rpm": "^7.6.1",
    "@electron-forge/maker-squirrel": "^7.6.1",
    "@electron-forge/maker-zip": "^7.6.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.1",
    "@electron-forge/plugin-fuses": "^7.6.1",
    "@electron-forge/plugin-webpack": "^7.6.1",
    "@electron/fuses": "^1.8.0",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-onboarding": "^8.5.3",
    "@storybook/addon-webpack5-compiler-swc": "^2.0.0",
    "@storybook/blocks": "^8.5.3",
    "@storybook/react": "^8.5.3",
    "@storybook/react-webpack5": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "ajv": "^8.12.0",
    "css-loader": "^6.0.0",
    "electron": "34.0.2",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-storybook": "^0.11.2",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "husky": "^9.1.7",
    "node-loader": "^2.0.0",
    "storybook": "^8.5.3",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4"
  },
  "keywords": [],
  "author": {
    "name": "Aram Hammoudeh",
    "email": "aram.devdocs@gmail.com"
  },
  "license": "MIT",
